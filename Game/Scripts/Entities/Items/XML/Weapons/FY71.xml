					<hint x="1.00" 	y="0.95" />
					<hint x="0.75"	y="1.00" />		
					<hint x="0.95" 	y="0.75" />
					<hint x="1.00" 	y="0.85" />
				</hints>
			</recoil>	
			<spread>
				<param name="min" value="1.15" />
