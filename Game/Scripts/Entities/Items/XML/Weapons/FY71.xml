				<param name="impulse" value="80" />
				<param name="delay" value="0.15" />
				<param name="duration" value="0.35" />
			</melee>
			<actions>
				<param name="attack" value="melee" />
			</actions>
		</firemode>
		<firemode name="GrenadeLauncher" type="Single" enabled="0">
